<%=stylesheet_link_tag 'bootstrap'%>
<link href="https://fonts.googleapis.com/css?family=Righteous" rel="stylesheet">
<section class="marketplace-lander">
  <%=form_tag '/s', id: 'search_form1', method: :get do%>
    <div class="coverimage">
      <figure class="marketplace-cover fluidratio display-none-at-small" style="background-image: url('<%=Community.last.cover_photo.url(:original)%>');background-size: 100% 100%;
        height: 376px;">
        <div class="lander-content marketplace-lander-content">
          <p class="marketplace-lander-content-description"></p>
          <div class="search-form margin-top-form">
            <div class="search-form-input search-form-text-field">
              <input type="text" name="q" placeholder="<%=(@community_customization && @community_customization.search_placeholder) || t("homepage.index.what_do_you_need")%>">
            </div>
            <div class="search-form-input">
              <button class="action-button" type="submit">
              <i class="ss-search icon-part"></i>
              </button>
            </div>
          </div>
        </div>
      </figure>
    </div>
  <%end%>
  <%=form_tag '/s', id: 'search_form2', method: :get do%>
    <div class="coverimage">
      <figure class="marketplace-cover fluidratio display-none-at-big" style="background-image: url('assets/Jakokulma_web_mobile.jpg');background-size: 100% 100%; height: 350px;">
        <div class="lander-content marketplace-lander-content">
          <p class="marketplace-lander-content-description"></p>
          <div class="search-form margin-top-form">
            <div class="search-form-input search-form-text-field">
              <input type="text" name="q" placeholder="<%=(@community_customization && @community_customization.search_placeholder) || t("homepage.index.what_do_you_need")%>">
            </div>
            <div class="search-form-input">
              <button class="action-button" type="submit">
              <i class="ss-search icon-part"></i>
              </button>
            </div>
          </div>
        </div>
      </figure>
    </div>
  <%end%>
</section>
